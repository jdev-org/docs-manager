    "version": "0.2.0",
    "configurations": [
        {
            "type": "java",
            "name": "Current File",
            "request": "launch",
            "mainClass": "${file}"
        },
        {
            "type": "java",
            "name": "Application",
            "request": "launch",
            "mainClass": "org.georchestra.docsmanager.Application",
            "projectName": "docsmanager",
            "args": "--spring.devtools.restart.enabled=true --spring.config.location=/srv/docs-manager/application.properties"
        }
    ]
}